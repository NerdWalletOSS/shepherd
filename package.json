{
  "name": "@nerdwallet/shepherd",
  "version": "1.14.1",
  "description": "A utility for applying code changes across many repositories",
  "keywords": [
    "codemod",
    "codemods",
    "migration"
  ],
  "homepage": "https://github.com/NerdWalletOSS/shepherd#readme",
  "bugs": {
    "url": "https://github.com/NerdWalletOSS/shepherd/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NerdWalletOSS/shepherd.git"
  },
  "license": "Apache-2.0",
  "author": "Nathan Walters",
  "bin": {
    "shepherd": "./lib/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "npm run build -- --watch",
    "fix-lint": "npm run lint -- --fix",
    "lint": "eslint src/**/*.ts",
    "prepublishOnly": "npm run test && npm run build",
    "test": "jest --coverage src/"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "@octokit/rest": "^18.9.0",
    "@types/js-yaml": "^3.12.7",
    "chalk": "^4.1.2",
    "child-process-promise": "^2.2.1",
    "commander": "^6.2.1",
    "fs-extra": "^9.1.0",
    "joi": "^17.4.2",
    "js-yaml": "^3.14.1",
    "lodash": "^4.17.21",
    "log-symbols": "^4.1.0",
    "netrc": "^0.1.4",
    "ora": "^5.4.1",
    "preferences": "^2.0.2",
    "simple-git": "^2.42.0"
  },
  "devDependencies": {
    "@octokit/types": "^6.24.0",
    "@octokit/plugin-rest-endpoint-methods": "^5.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.172",
    "@types/log-symbols": "^2.0.0",
    "@types/node": "^14.17.9",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jest": "^26.6.3",
    "semantic-release": "^17.4.4",
    "ts-jest": "^26.5.6",
    "typescript": "^3.9.10"
  },
  "publishConfig": {
    "access": "public"
  }
}
